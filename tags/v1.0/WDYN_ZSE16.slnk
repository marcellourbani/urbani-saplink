endmethod.
" ACTION_TYPE="0" ABAP_TYPING="0"/>
  <wdy_ctlr_compo COMPONENT_NAME="ZSE16" CONTROLLER_NAME="SELECTION" CMPNAME="ONACTIONRUNQUERY" VERSION="I" CMPTYPE="CL_WDY_MD_CTLR_EVENT_HANDLER" CMP_POSITION="6 " VISIBILITY="2" REF_CTLR_NAME="SELECTION" REF_CMPNAME="RUNQUERY" DISPLAY_NAME="ONACTIONRUNQUERY" CODE_BODY="method onactionrunquery .
  data: sel_fields type if_wd_select_options=&gt;tt_selection_screen_item,
        sel_field  type if_wd_select_options=&gt;t_selection_screen_item,
        tab type ref to data.
  try.
      wd_this-&gt;sel_handler-&gt;get_selection_fields( importing et_fields = sel_fields ).
      wd_this-&gt;tabdata-&gt;clear_sel( ).
      loop at sel_fields into sel_field.
        wd_this-&gt;tabdata-&gt;add_sel( id = sel_field-m_id rangetab = sel_field-mt_range_table ).
      endloop.
      wd_this-&gt;tabdata-&gt;exec_query( ).
      tab = wd_this-&gt;tabdata-&gt;get_data( ).
      wd_this-&gt;fire_to_alv_plg( tab = tab tabcl = wd_this-&gt;tabdata ).
    catch cx_root.
      data: l_current_controller type ref to if_wd_controller,
            l_message_manager    type ref to if_wd_message_manager.

      l_current_controller ?= wd_this-&gt;wd_get_api( ).

      call method l_current_controller-&gt;get_message_manager
        receiving
          message_manager = l_message_manager.
      data: l_text_e01 type string.
      l_text_e01 =  wd_assist-&gt;if_wd_component_assistance~get_text( &apos;E01&apos; ).

      call method l_message_manager-&gt;report_error_message
        exporting
          message_text             = l_text_e01.


  endtry.
endmethod.
