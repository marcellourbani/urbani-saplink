  <controller_definition COMPONENT_NAME="ZSE16" CONTROLLER_NAME="SELECTION" VERSION="A" CONTROLLER_TYPE="01" CONTEXT="CONTEXT" DISPLAY_NAME="SELECTION" AUTHOR="BCUSER" CREATEDON="20071102" CHANGEDBY="BCUSER" CHANGEDON="20071118">
   <wdy_controllert COMPONENT_NAME="ZSE16" CONTROLLER_NAME="SELECTION" LANGU="E" DESCRIPTION="selection screen"/>
   <wdy_ctlr_compo COMPONENT_NAME="ZSE16" CONTROLLER_NAME="SELECTION" CMPNAME="ADD_SELOPT" VERSION="I" CMPTYPE="CL_WDY_MD_CONTROLLER_METHOD" CMP_POSITION="1 " VISIBILITY="2" DISPLAY_NAME="ADD_SELOPT" CODE_BODY="method ADD_SELOPT .
  data:lt_range_table type ref to data,
       l_type type string,
       l_name type string.
  l_type = field-rollname.
  l_name = field-fieldname.
  lt_range_table = wd_this-&gt;sel_handler-&gt;create_range_table( i_typename = l_type ).
  if with_descriptions = abap_true.
    wd_this-&gt;sel_handler-&gt;add_selection_field(
                       i_id = l_name
                       it_result = lt_range_table ).
  else.
    wd_this-&gt;sel_handler-&gt;add_selection_field(
                       i_id = l_name
                       i_description = l_name
                       it_result = lt_range_table ).
  endif.
endmethod.
