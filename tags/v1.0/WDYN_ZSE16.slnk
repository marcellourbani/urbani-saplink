" ACTION_TYPE="0" ABAP_TYPING="0"/>
   <wdy_ctlr_compo COMPONENT_NAME="ZSE16" CONTROLLER_NAME="SELECTION" CMPNAME="HANDLEFROM_TABLESEL" VERSION="I" CMPTYPE="CL_WDY_MD_CTLR_EVENT_HANDLER" CMP_POSITION="4 " VISIBILITY="2" REF_VIEW_NAME="SELECTION" REF_PLUG_NAME="FROM_TABLESEL" DISPLAY_NAME="HANDLEFROM_TABLESEL" CODE_BODY="method handlefrom_tablesel .
  data:newtab type abap_bool,
       field type dfies,
       elem_context type ref to if_wd_context_element,
       stru_context type wd_this-&gt;element_context.
  if wd_this-&gt;tabdata is bound.
    if wd_this-&gt;tabdata-&gt;tabname &lt;&gt; nome.
      newtab = abap_true.
    endif.
  else.
    newtab = abap_true.
  endif.

  if newtab = abap_true.
    wd_this-&gt;sel_handler-&gt;remove_all_sel_screen_items( ).
    try.
        create object wd_this-&gt;tabdata
          exporting
            name = nome.
        loop at wd_this-&gt;tabdata-&gt;userfields into field.
*          wd_this-&gt;tabdata-&gt;add_selopt( selopt = wd_this-&gt;sel_handler field = field ).
          add_selopt( field = field with_descriptions = abap_true ).
        endloop.
        elem_context = wd_context-&gt;get_element(  ).
        stru_context-tabname = wd_this-&gt;tabdata-&gt;tabname.
        stru_context-tabdesc = wd_this-&gt;tabdata-&gt;tabdesc.
        elem_context-&gt;set_attribute(
            exporting
              name =  `TABNAME`
              value = stru_context-tabname ).
        elem_context-&gt;set_attribute(
            exporting
              name =  `TABDESC`
              value = stru_context-tabdesc ).
      catch cx_sy_create_object_error.
    endtry.
  endif.
endmethod.
" ACTION_TYPE="0" ABAP_TYPING="0"/>
   <wdy_ctlr_compo COMPONENT_NAME="ZSE16" CONTROLLER_NAME="SELECTION" CMPNAME="ONACTIONBACKTOSELTAB" VERSION="I" CMPTYPE="CL_WDY_MD_CTLR_EVENT_HANDLER" CMP_POSITION="5 " VISIBILITY="2" REF_CTLR_NAME="SELECTION" REF_CMPNAME="BACKTOSELTAB" DISPLAY_NAME="ONACTIONBACKTOSELTAB" CODE_BODY="method ONACTIONBACKTOSELTAB .
    wd_this-&gt;fire_to_tablesel_plg( ).
endmethod.
" ACTION_TYPE="0" ABAP_TYPING="0"/>
   <wdy_ctlr_compo COMPONENT_NAME="ZSE16" CONTROLLER_NAME="SELECTION" CMPNAME="ONACTIONRUNQUERY" VERSION="I" CMPTYPE="CL_WDY_MD_CTLR_EVENT_HANDLER" CMP_POSITION="6 " VISIBILITY="2" REF_CTLR_NAME="SELECTION" REF_CMPNAME="RUNQUERY" DISPLAY_NAME="ONACTIONRUNQUERY" CODE_BODY="method onactionrunquery .
  data: sel_fields type if_wd_select_options=&gt;tt_selection_screen_item,
        sel_field  type if_wd_select_options=&gt;t_selection_screen_item,
        tab type ref to data.
  try.
      wd_this-&gt;sel_handler-&gt;get_selection_fields( importing et_fields = sel_fields ).
      wd_this-&gt;tabdata-&gt;clear_sel( ).
      loop at sel_fields into sel_field.
        wd_this-&gt;tabdata-&gt;add_sel( id = sel_field-m_id rangetab = sel_field-mt_range_table ).
      endloop.
      wd_this-&gt;tabdata-&gt;exec_query( ).
      tab = wd_this-&gt;tabdata-&gt;get_data( ).
      wd_this-&gt;fire_to_alv_plg( tab = tab tabcl = wd_this-&gt;tabdata ).
    catch cx_root.
      data: l_current_controller type ref to if_wd_controller,
            l_message_manager    type ref to if_wd_message_manager.

      l_current_controller ?= wd_this-&gt;wd_get_api( ).

      call method l_current_controller-&gt;get_message_manager
        receiving
          message_manager = l_message_manager.
      data: l_text_e01 type string.
      l_text_e01 =  wd_assist-&gt;if_wd_component_assistance~get_text( &apos;E01&apos; ).

      call method l_message_manager-&gt;report_error_message
        exporting
          message_text             = l_text_e01.


  endtry.
endmethod.
