   <wdy_ctlr_compo COMPONENT_NAME="ZSE16" CONTROLLER_NAME="SELECTION" CMPNAME="BACKTOSELTAB" VERSION="I" CMPTYPE="CL_WDY_MD_ACTION" CMP_POSITION="2 " VISIBILITY="0" DISPLAY_NAME="BACKTOSELTAB" ACTION_TYPE="0" ABAP_TYPING="0"/>
   <wdy_ctlr_compo COMPONENT_NAME="ZSE16" CONTROLLER_NAME="SELECTION" CMPNAME="HANDLEFROM_ALV" VERSION="I" CMPTYPE="CL_WDY_MD_CTLR_EVENT_HANDLER" CMP_POSITION="3 " VISIBILITY="2" REF_VIEW_NAME="SELECTION" REF_PLUG_NAME="FROM_ALV" DISPLAY_NAME="HANDLEFROM_ALV" CODE_BODY="method HANDLEFROM_ALV .
